</head>
<body>
  <header>
    <div class="navbar navbar-default navbar-fixed-top ">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="../"><%= @setting[:title] %></a>

          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <!--<a href="../about">About</a>-->
            <% if current_user %>
            <li><a href="/admin">Admin</a></li>
            <li><a href="#/new">New Entry</a></li>
            <li><a href="#"><%= current_user.name %></a></li>
            <li><%= link_to "Sign Out", signout_path %></li>
            <% else %>
            <li><%= link_to "Sign in with Google", "/auth/open_id?openid_url=https://www.google.com/accounts/o8/id" %></li>
            <% end %>
          </ul>
          <!--<form class="navbar-search pull-right" action="/search" method="get">
            <input type="text" class="search-query" placeholder="Search..." id="search" name="q" value="" />
          </form> -->
        </div> <!-- nav-collapse -->

      </div> <!-- container -->
    </div> <!-- navbar navbar-fixed-top -->
  </header>

  <div class="container">
    <div class="row">
      <div class="col-sm-8">
        <%= yield %>
      </div>
      <div class="col-sm-4 sidebar">

        <div>
          <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
          <ins class="adsbygoogle"
               style="display:inline-block;width:320px;height:100px"
               data-ad-client="ca-pub-4800068521558938"
               data-ad-slot="7481760013"></ins>
          <script>
          (adsbygoogle = window.adsbygoogle || []).push({});
          </script>
        </div>

        <h3>Tag</h3>
        <div id="tag_cloud">
          <%
            @tag_cloud[0..30].each_with_index do |tag, i|
              css = "tag4"
              case i
              when 0..4   then css = "tag1"
              when 5..9   then css = "tag2"
              when 10..14 then css = "tag3"
              end
           %>
           <%= link_to "#{tag[0]}(#{tag[1]})", "/posts/tag/#{tag[0]}", :class => css %>
          <% end %>
        </div>
      </div><!-- sidebar -->
    </div>
  </div>

  <%= @setting[:ga_tracking_code].html_safe %>

  <!-- ======== SCRIPTS ======== -->


